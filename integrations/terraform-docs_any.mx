summary terraform-docs "A utility to generate documentation from Terraform modules in various output formats"

autocomplete set terraform-docs %[
    {
        DynamicDesc: '{
            cast json
            config set proc strict-arrays false
            terraform-docs @PARAMS --help -> tabulate --key-value --map --split-comma --key-inc-hint
        }'
        AllowMultiple: true
        AllowAny:      true
        ListView:      true
    }
]
